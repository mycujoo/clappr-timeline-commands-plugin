{
  "name": "clappr-timeline-commands-plugin",
  "version": "0.0.1",
  "description": "A plugin for clappr which executes functions when the timeline hits a time.",
  "main": "dist/clappr-timeline-commands-plugin.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack --progress --optimize-occurence-order",
    "watch": "./node_modules/.bin/webpack --progress --optimize-occurence-order --watch",
    "release": "./node_modules/.bin/webpack --progress --optimize-occurence-order -p"
  },
  "author": "Gabriel Plegge <gplegge@gmail.com>",
  "license": "MIT",
  "repository": "mycujoo/clappr-timeline-commands-plugin",
  "devDependencies": {
    "babel-core": "^6.7.5",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.6.0",
    "clean-webpack-plugin": "^0.1.8",
    "webpack": "^1.12.14"
  }
}
